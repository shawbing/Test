<resources>

    <!-- Base application theme. -->

    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Share.ToolBar" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="android:background">@color/colorPrimary</item>
        <item name="android:text">@color/colorTitleText</item>
        <item name="navigationIcon">?attr/homeAsUpIndicator</item>
    </style>

    <style name="perference_set_activity">
        <item name="android:windowBackground">@color/colorBack</item>  <!-- 窗口背景 -->
        <item name="android:focusable">false</item>
        <!--      <item name="android:textColor">@color/colorBack</item>  字体颜色-->
        <item name="android:colorBackgroundCacheHint">#00000000</item> <!-- 背景缓存色透明，避免拖动全黑 -->
        <item name="android:windowNoTitle">true</item> <!-- 是否有标题 -->
    </style>

</resources>
